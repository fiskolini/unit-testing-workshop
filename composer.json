{
    "name": "exads/be-unit-testing-workshop",
    "description": "BE Workshop around the Unit Test",
    "type": "project",
    "authors": [
        {
            "name": "Francisco Carvalho",
            "email": "fiskolini@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "aura/router": "^3.1",
        "laminas/laminas-diactoros": "^2.4",
        "psr/http-server-handler": "^1.0",
        "php-di/php-di": "^6.2",
        "doctrine/dbal": "^2.10",
        "beberlei/assert": "^3.2",
        "symfony/mailer": "^5.1",
        "doctrine/orm": "^2.7",
        "symfony/filesystem": "^5.1",
        "symfony/cache": "^5.4",
        "doctrine/annotations": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    }
}
